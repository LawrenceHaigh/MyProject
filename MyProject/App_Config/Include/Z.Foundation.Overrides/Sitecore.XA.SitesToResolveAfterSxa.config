<?xml version="1.0" encoding="utf-8" ?>
<!--

Purpose: This include file adds proper attribute to site definitions in order to resolve sites after SXA sites

Patch for sites definitions

To disable this file, change its extension to ".disabled".

-->
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <experienceAccelerator>
            <siteResolving>
                <site name="exm" resolve="after" />
                <site name="website" resolve="after" />
            </siteResolving>
        </experienceAccelerator>
    </sitecore>
</configuration>

