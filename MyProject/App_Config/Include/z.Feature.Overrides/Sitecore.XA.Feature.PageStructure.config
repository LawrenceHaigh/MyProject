<?xml version="1.0" encoding="utf-8" ?>
<!--

Purpose: This include file enables and configures the Page Structure feature.

This feature structures the page by inserting columns, rows, and containers. 

To disable this file, change its extension to ".disabled".

-->
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <getGridParametersDialogFields>
        <processor patch:before="*[@type='Sitecore.XA.Foundation.Grid.Pipelines.GetGridParametersDialogFields.FilterDuplicatedFieldDescriptors, Sitecore.XA.Foundation.Grid']" type="Sitecore.XA.Feature.PageStructure.Pipelines.GetGridParametersDialogFields.RemoveRedundantSplitterFieldsDescriptors, Sitecore.XA.Feature.PageStructure" />
      </getGridParametersDialogFields>
    </pipelines>
  </sitecore>
</configuration>

